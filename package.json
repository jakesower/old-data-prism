    "@cycle/http": "^15.1.0",
    "@cycle/isolate": "^3.4.0",
    "@cycle/run": "^5.1.0",
    "@cycle/storage": "^5.0.3",
    "csv-parse": "^1.2.0",
    "csv-stringify": "^5.1.1",
    "file-saver": "^2.0.0",
    "js-sha1": "^0.6.0",
    "marked": "^0.3.6",
    "mathjs": "^5.2.3",
    "moment": "^2.23.0",
    "ramda": "^0.25.0",
    "xstream": "^11.7.0"
  }
