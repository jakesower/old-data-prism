{
  "name": "data-prism",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "Jake Sower",
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "build-js": "webpack",
    "watch-js": "webpack --watch --mode=development",
    "build-sass": "mkdirp dist/styles && node-sass -r assets/styles/main.scss --output-style=compressed -o dist/styles",
    "watch-sass": "mkdirp dist/styles && node-sass -wr assets/styles/main.scss --output-style=compressed -o dist/styles",
    "watch": "concurrently \"npm run watch-js\" \"npm run watch-sass\"",
    "test": "find ./test -name '*.spec.js' | CHECK_TYPES=true xargs mocha -R spec --reporter nyan",
    "test-check": "find ./test -name '*.spec.js' | CHECK_TYPES=true TESTCHECK=true xargs mocha -t 50000 -R spec --reporter nyan"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.2.1",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@cycle/dom": "^21.1.0",
    "@cycle/isolate": "^3.4.0",
    "@cycle/most-run": "^8.0.0",
    "@cycle/storage": "^5.0.3",
    "csv-parse": "^1.2.0",
    "csv-stringify": "^1.0.4",
    "file-saver": "^1.3.3",
    "marked": "^0.3.6",
    "mathjs": "^3.16.3",
    "moment": "^2.22.2",
    "most": "^1.7.3",
    "most-proxy": "^3.3.0",
    "xstream": "^11.7.0"
  }
}
