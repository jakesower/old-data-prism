    "mathjs": "^3.16.3",
    "moment": "^2.18.1",
    "most": "^1.7.3",
    "most-proxy": "^3.3.0",
    "ramda": "^0.24.0",
    "sanctuary": "^0.12.2",
    "sanctuary-def": "^0.12.1",
