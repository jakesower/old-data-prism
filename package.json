    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "node-sass": "^4.9.3",
    "source-map-loader": "^0.2.4",
    "testcheck": "^1.0.0-rc.2",
    "typescript": "^3.1.2",
    "watchify": "^3.7.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
